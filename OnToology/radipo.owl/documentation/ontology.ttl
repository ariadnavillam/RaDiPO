#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#Ability_affected
:Ability_affected rdf:type owl:Class ;
                  rdfs:subClassOf [ rdf:type owl:Restriction ;
                                    owl:onProperty :is_ability_affected_by ;
                                    owl:someValuesFrom :Disability
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :ability_specification ;
                                    owl:someValuesFrom xsd:string
                                  ] ;
                  rdfs:comment "Abilities, task and activities a patient has limitations doing." ;
                  rdfs:label "Ability_affected" .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#Biological_anomaly
:Biological_anomaly rdf:type owl:Class ;
                    rdfs:subClassOf :Disorder .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#Biological_process
:Biological_process rdf:type owl:Class ;
                    rdfs:subClassOf [ rdf:type owl:Restriction ;
                                      owl:onProperty :has_protein_involved ;
                                      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onClass :Protein
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :has_process_id ;
                                      owl:someValuesFrom xsd:string
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :has_process_name ;
                                      owl:someValuesFrom xsd:string
                                    ] ;
                    rdfs:comment "Human processes or metabolic rutes the protein is involved in. The ids and names are terms from the Gene Ontology." ;
                    rdfs:label "Biological_process" .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#Biotech
:Biotech rdf:type owl:Class ;
         rdfs:subClassOf :Drug ;
         owl:disjointWith :Small_molecule ;
         rdfs:comment "These are drugs with a biological origin. This term includes vaccines, blood components, allergenics, somatic cells, gene therapies, tissues, recombinant therapeutic protein, and living cells used in cell therapy." ;
         rdfs:label "Biotech" .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#Cellular_component
:Cellular_component rdf:type owl:Class ;
                    rdfs:subClassOf [ rdf:type owl:Restriction ;
                                      owl:onProperty :has_protein_located ;
                                      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onClass :Protein
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :has_cell_id ;
                                      owl:someValuesFrom xsd:string
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :has_cell_name ;
                                      owl:someValuesFrom xsd:string
                                    ] ;
                    rdfs:comment "Location of a cell where the protein is located. The ids and names are terms from the Gene Ontology." ;
                    rdfs:label "Cellular_component" .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#Clinical_entity
:Clinical_entity rdf:type owl:Class ;
                 rdfs:subClassOf [ rdf:type owl:Restriction ;
                                   owl:onProperty :has_symptom ;
                                   owl:someValuesFrom :Symptom
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :is_associated_to_disability ;
                                   owl:someValuesFrom :Disability
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :is_associated_to_gene ;
                                   owl:someValuesFrom :Genetic_entity
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :has_disorder_description ;
                                   owl:someValuesFrom owl:rational
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :has_disorder_id ;
                                   owl:someValuesFrom xsd:string
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :has_disorder_name ;
                                   owl:someValuesFrom xsd:string
                                 ] ;
                 rdfs:comment "Genetic technical term used to describe the clinical items included in the nomenclature of rare diseases. The id is ORPHAcode." ;
                 rdfs:label "Clinical_entity" .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#Clinical_group
:Clinical_group rdf:type owl:Class ;
                rdfs:subClassOf :Group_of_disorders .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#Clinical_subtype
:Clinical_subtype rdf:type owl:Class ;
                  rdfs:subClassOf :Subtype_of_disorder .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#Clinical_syndrome
:Clinical_syndrome rdf:type owl:Class ;
                   rdfs:subClassOf :Disorder .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#Degree_of_severity
:Degree_of_severity rdf:type owl:Class ;
                    rdfs:subClassOf [ rdf:type owl:Restriction ;
                                      owl:onProperty :degree_from_disability ;
                                      owl:someValuesFrom :Disability
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :has_name ;
                                      owl:someValuesFrom xsd:string
                                    ] ;
                    rdfs:comment "The degree to which the ability is affected in the patient." ;
                    rdfs:label "Degree_of_severity" .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#Disability
:Disability rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :affects_ability ;
                              owl:someValuesFrom :Ability_affected
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :has_degree_of_severity ;
                              owl:someValuesFrom :Degree_of_severity
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :has_temporality ;
                              owl:someValuesFrom :Temporality
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :is_consequence_of_disorder ;
                              owl:someValuesFrom :Clinical_entity
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :produces_loss_of_ability ;
                              owl:someValuesFrom :Loss_of_ability
                            ] ;
            rdfs:comment "The functional consequences of a disorder are divided into different abilities, task, and activities of the everyday life. Disability involves limitation or restriction in a task." ;
            rdfs:label "Disability" .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#Disease
:Disease rdf:type owl:Class ;
         rdfs:subClassOf :Disorder .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#Disorder
:Disorder rdf:type owl:Class ;
          rdfs:subClassOf :Clinical_entity ;
          rdfs:comment "A disorder can be a disease, a symdrom or an anomaly." ;
          rdfs:label "Disorder" .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#Drug
:Drug rdf:type owl:Class ;
      rdfs:subClassOf [ rdf:type owl:Restriction ;
                        owl:onProperty :has_target ;
                        owl:someValuesFrom :Protein
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :is_in_phase ;
                        owl:someValuesFrom :Experimental_phase
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :has_drug_description ;
                        owl:someValuesFrom xsd:string
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :has_drug_id ;
                        owl:someValuesFrom xsd:string
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :has_drug_indication ;
                        owl:someValuesFrom xsd:string
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :has_drug_name ;
                        owl:someValuesFrom xsd:string
                      ] ;
      rdfs:comment "A molecular compound that is used as a treatment for a disease involving a protein. The ids and names are from the DrugBank." ;
      rdfs:label "Drug" .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#Enzyme
