#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/rights
<http://purl.org/dc/elements/1.1/rights> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
<http://purl.org/dc/elements/1.1/title> rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#type_of_association
:type_of_association rdf:type owl:AnnotationProperty ;
                     rdfs:range xsd:string ;
                     rdfs:domain :is_associated_to_disorder .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#ability_is_lost_in
:ability_is_lost_in rdf:type owl:ObjectProperty ;
                    owl:inverseOf :produces_loss_of_ability ;
                    rdfs:domain :Loss_of_ability ;
                    rdfs:range :Disability .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#affects_ability
:affects_ability rdf:type owl:ObjectProperty ;
                 owl:inverseOf :is_ability_affected_by ;
                 rdfs:domain :Ability_affected ;
                 rdfs:range :Disability .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#degree_from_disability
:degree_from_disability rdf:type owl:ObjectProperty ;
                        owl:inverseOf :has_degree_of_severity ;
                        rdfs:domain :Degree_of_severity ;
                        rdfs:range :Disability .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#had_by_protein
:had_by_protein rdf:type owl:ObjectProperty ;
                owl:inverseOf :has_function ;
                rdfs:domain :Function ;
                rdfs:range :Protein .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#has_biological_process
:has_biological_process rdf:type owl:ObjectProperty ;
                        owl:inverseOf :has_protein_involved ;
                        rdfs:domain :Protein ;
                        rdfs:range :Biological_process .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#has_cellular_component
:has_cellular_component rdf:type owl:ObjectProperty ;
                        owl:inverseOf :has_protein_located ;
                        rdfs:domain :Protein ;
                        rdfs:range :Cellular_component .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#has_degree_of_severity
:has_degree_of_severity rdf:type owl:ObjectProperty ;
                        rdfs:domain :Disability ;
                        rdfs:range :Degree_of_severity .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#has_function
:has_function rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:domain :Protein ;
              rdfs:range :Function .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#has_protein_involved
:has_protein_involved rdf:type owl:ObjectProperty ;
                      rdfs:domain :Biological_process ;
                      rdfs:range :Protein .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#has_protein_located
:has_protein_located rdf:type owl:ObjectProperty ;
                     rdfs:domain :Cellular_component ;
                     rdfs:range :Protein .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#has_protein_product
:has_protein_product rdf:type owl:ObjectProperty ;
                     owl:inverseOf :is_encoded_by ;
                     rdfs:domain :Gene_with_protein ;
                     rdfs:range :Protein .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#has_symptom
:has_symptom rdf:type owl:ObjectProperty ;
             owl:inverseOf :is_present_in ;
             rdfs:domain :Clinical_entity ;
             rdfs:range :Symptom .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#has_target
:has_target rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            owl:inverseOf :has_target ;
            rdfs:domain :Drug ;
            rdfs:range :Protein .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#has_temporality
:has_temporality rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 owl:inverseOf :is_related_to_disability ;
                 rdfs:domain :Disability ;
                 rdfs:range :Temporality .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#is_ability_affected_by
:is_ability_affected_by rdf:type owl:ObjectProperty ;
                        rdfs:domain :Ability_affected ;
                        rdfs:range :Disability .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#is_associated_to_disability
:is_associated_to_disability rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf owl:topObjectProperty ;
                             owl:inverseOf :is_consequence_of_disorder ;
                             rdfs:domain :Clinical_entity ;
                             rdfs:range :Disability .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#is_associated_to_disorder
:is_associated_to_disorder rdf:type owl:ObjectProperty ;
                           owl:inverseOf :is_associated_to_gene ;
                           rdfs:domain :Genetic_entity ;
                           rdfs:range :Clinical_entity ;
                           :type_of_association xsd:string .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#is_associated_to_gene
:is_associated_to_gene rdf:type owl:ObjectProperty ;
                       rdfs:domain :Clinical_entity ;
                       rdfs:range :Genetic_entity ;
                       :type_of_association xsd:string .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#is_consequence_of_disorder
:is_consequence_of_disorder rdf:type owl:ObjectProperty ;
                            rdfs:domain :Disability ;
                            rdfs:range :Clinical_entity .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#is_encoded_by
:is_encoded_by rdf:type owl:ObjectProperty ;
               rdfs:domain :Protein ;
               rdfs:range :Gene_with_protein .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#is_in_phase
:is_in_phase rdf:type owl:ObjectProperty ;
             rdfs:domain :Drug ;
             rdfs:range :Experimental_phase .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#is_present_in
:is_present_in rdf:type owl:ObjectProperty ;
               rdfs:domain :Symptom ;
               rdfs:range :Clinical_entity .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#is_related_to_disability
:is_related_to_disability rdf:type owl:ObjectProperty ;
                          rdfs:domain :Temporality ;
                          rdfs:range :Disability .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#is_target_of
:is_target_of rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/usuario/ontologies/2022/0/RaDiPO#produces_loss_of_ability
:produces_loss_of_ability rdf:type owl:ObjectProperty ;
                          rdfs:domain :Disability ;
                          rdfs:range :Loss_of_ability .


